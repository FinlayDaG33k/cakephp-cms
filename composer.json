{
    "name": "qobo/cakephp-cms",
    "description": "Cms plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "cakephp/cakephp": "~3.3.0",
        "fzaninotto/faker": "*",
        "muffin/slug": "1.1.0",
        "maiconpinto/cakephp-adminlte-theme": "^1.0"
        "qobo/cakephp-utils": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Cms\\": "src",
            "Cms\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cms\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
