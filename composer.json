{
    "name": "qobo/cakephp-cms",
    "description": "Cms plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "fzaninotto/faker": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Cms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cms\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
