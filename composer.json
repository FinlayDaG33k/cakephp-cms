{
    "name": "qobo/cakephp-cms",
    "description": "Cms plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "cakedc/users": "^4.0",
        "hashmode/cakephp-tinymce-elfinder": "^1.0",
        "muffin/slug": "1.1.0",
        "qobo/cakephp-utils": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Cms\\": "src",
            "Cms\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cms\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-update-cmd": "CakephpTinymceElfinder\\Console\\Installer::postUpdate"
    },
}
